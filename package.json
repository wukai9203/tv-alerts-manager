{
  "name": "tv-alerts-manager",
  "version": "1.0.0",
  "description": "TradingView Alerts Manager",
  "main": "content.js",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "vite",
    "build": "node --experimental-vm-modules node_modules/vite/bin/vite.js build --config vite.config.js",
    "generate-icons": "node --experimental-vm-modules scripts/generate-icons.js",
    "convert-to-svg": "node --experimental-vm-modules scripts/convert-to-svg.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.{js,html,css}\"",
    "docs": "jsdoc -c jsdoc.json"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/user-event": "^14.5.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.4.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdoc": "^4.0.2",
    "jimp": "^0.22.10",
    "potrace": "^2.1.8",
    "prettier": "^3.0.3",
    "sharp": "^0.32.1",
    "vite": "^4.4.9",
    "vite-plugin-chrome-extension": "^0.0.7"
  },
  "dependencies": {
    "i18next": "^23.7.6",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.2.1",
    "jszip": "^3.10.1",
    "sentry-browser": "^7.77.0",
    "web-vitals": "^3.5.0"
  }
}
